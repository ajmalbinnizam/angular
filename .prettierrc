{
  "semi": false,
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 95,
  "endOfLine": "auto",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "trailingComma": "none",
  "quoteProps": "as-needed",
  "jsxBracketSameLine": false,
  
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
